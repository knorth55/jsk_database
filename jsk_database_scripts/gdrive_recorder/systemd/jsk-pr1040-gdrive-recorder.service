[Unit]
Description=JSK PR1040 gdrive recorder 
After=network-online.target
Wants=network-online.target

[Service]
User=applications
Group=applications
ExecStart=/bin/bash -c "source /home/applications/ros/melodic/devel/setup.bash && rossetip && rossetmaster pr1040 && rosrun jsk_tools roscore_regardless.py --respawn rosrun gdrive_recorder pr2_gdrive_recorder.py _video_path:=/tmp _database:=pr1040"

[Install]
WantedBy=multi-user.target
